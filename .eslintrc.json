{
	"extends": [
		"airbnb-base",
		"plugin:@wordpress/eslint-plugin/recommended",
		"prettier"
	],
	"plugins": [ "import", "prettier" ],
	"rules": {
		"prettier/prettier": "error",
		"max-len": [ "error", 80, { "ignoreStrings": true } ],
		"import/no-extraneous-dependencies": [ "off" ],
		"no-console": "off",
		"import/prefer-default-export": "off"
	},
	"env": {
		"node": true
	}
}
